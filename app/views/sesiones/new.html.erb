<h1>Iniciar Sesión</h1>

<%= form_with url: login_path, method: :post, local: true do %>
  <div>
    <%= label_tag :correo %><br>
    <%= text_field_tag :correo %>
  </div>
  <div>
    <%= label_tag :contrasena, "Contraseña" %><br>
    <%= password_field_tag :contrasena %>
  </div>
  <%= submit_tag "Ingresar" %>
<% end %>
